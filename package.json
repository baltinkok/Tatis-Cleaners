{
  "name": "tatis-cleaners",
  "version": "1.0.0",
  "private": true,
  "description": "Tati's Cleaners - Professional Cleaning Services Platform",
  "license": "MIT",
  "scripts": {
    "install-frontend": "cd frontend && yarn install --network-timeout 100000",
    "build-frontend": "cd frontend && yarn build",
    "start-frontend": "cd frontend && serve -s build -p $PORT -H 0.0.0.0",
    "build": "yarn install-frontend && yarn build-frontend",
    "start": "yarn start-frontend"
  },
  "dependencies": {
    "serve": "^14.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  }
}